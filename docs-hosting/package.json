{
  "name": "docs-hosting",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npm run build:rustdoc && npm run build:next",
    "build:next": "next build",
    "build:rustdoc": "cargo doc --no-deps --document-private-items --all-features && mkdir -p public/rustdoc && cp -r ../target/doc/* public/rustdoc",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "next": "latest",
    "next-auth": "^4.23.1",
    "react": "latest",
    "react-dom": "latest"
  },
  "devDependencies": {
    "@types/node": "latest",
    "@types/react": "latest",
    "@types/react-dom": "latest",
    "autoprefixer": "latest",
    "eslint": "latest",
    "eslint-config-next": "latest",
    "postcss": "latest",
    "tailwindcss": "latest",
    "typescript": "latest"
  }
}
